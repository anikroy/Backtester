import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class test {

	public static void main(String[] args) throws FileNotFoundException {
		// TODO Auto-generated method stub
		
		Scanner file = new Scanner(new File("C:/Users/Admin/Desktop/Backtester/opra_20130301_bar05/opra_20130301_bar05.txt"));
	    int count = 0;
	    while(file.hasNext()){
	    	file.nextLine();
	        count++;
	        if(count%10000==0){
	        	System.out.println(count);
	        }
	    }
	    file.close();
	    System.out.println(count);

	}

}
